{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "POSTGRES_URL": "@postgres-url",
    "POSTGRES_URL_NON_POOLING": "@postgres-url-non-pooling",
    "GEMINI_API_KEY": "@gemini-api-key"
  }
}
